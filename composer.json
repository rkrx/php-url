{
	"name": "rkr/url",
	"description": "Url builder",
	"license": "MIT",
	"authors": [{
		"name": "ron",
		"email": "ron.kirschler@gmail.com"
	}],
	"require": {
		"php": ">= 5.6.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.0",
		"phpstan/phpstan": ">=1.0"
	},
	"autoload": {
		"psr-4": {
			"Kir\\Url\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kir\\Url\\": ["src/", "tests/"]
		}
	},
	"scripts": {
		"phpstan": "vendor/bin/phpstan analyse --level=max src"
	}
}
